\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
p(z\mid x)=\phi(z;\sum_{i=1}^{k}w_{i}x_{i},\omega_{H})
\]

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\omega_{H}=(\sum_{i=1}^{k}\sigma_{i}^{-2}\lambda^{2})/(1+\sum_{i=1}^{k}\sigma_{i}^{-2}\lambda^{2})$
\end_inset

 is coefficient 
\begin_inset Formula $H$
\end_inset

 (ref) and 
\begin_inset Formula $v_{i}$
\end_inset

 are Thurstone weights.
\end_layout

\begin_layout Standard
Moreover,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
p(z\mid y)=Ep(z\mid x)p(x\mid y)
\]

\end_inset


\end_layout

\begin_layout Standard
And
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\phi(x\mid y)=\frac{\exp(-\frac{1}{2}x^{t}\Phi^{-1}x)}{\int_{B}\exp(-\frac{1}{2}x^{t}\Phi^{-1}x)dx}
\]

\end_inset

where 
\begin_inset Formula $B$
\end_inset

 is the region bounded by 
\begin_inset Formula $y$
\end_inset

 in the appropriate way.
 The density of 
\begin_inset Formula $\theta=\sum_{i=1}^{k}w_{i}x_{i}$
\end_inset

 is (use the affine transformation property on wikiepedia)
\end_layout

