\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Cov(\Xi)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E\hat{X}_{i}=E[E[\hat{X}_{i}\mid Y]]=
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
E\hat{X}_{i} & = & \sum_{k=1}^{m_{i}}w_{k}p_{ij}(k,l)=-\sum_{k=1}^{m_{i}}[\phi(\tau_{iY_{i}})-\phi(\tau_{i(Y_{i}-1)})]=0\\
E\hat{X}_{i}\hat{X}_{j} & = & E\sum_{k=1}^{m_{i}}w|_{k}^{i}1[Y_{i}=k]\sum_{k=1}^{m_{j}}w_{l}^{j}1[Y_{i}=l]\\
 & = & \sum_{i=1}^{k}\sum_{j=1}^{j}w_{k}^{i}w_{l}^{j}p_{ij}(k,l)
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\alpha_{s} & = & \frac{k}{k-1}\left(\frac{i^{T}\Phi i-k}{i^{T}\Phi i}\right)\\
 & = & k^{2}\left(\frac{\overline{\rho}}{i^{T}\Phi i}\right)
\end{eqnarray*}

\end_inset

Since 
\begin_inset Formula $\overline{\rho}=\lambda^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\lambda^{2}}{\left(k\lambda^{2}+\sigma^{2}\right)^{2}}=\alpha_{s}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\left(k\lambda^{2}+\sigma^{2}\right)^{2} & = & ((i^{T}\Phi i)/k)^{2}\\
1/\left(k\lambda^{2}+\sigma^{2}\right)^{2} & = & k^{2}/(i^{T}\Phi i)^{2}\\
\\
\frac{\lambda^{2}}{\left(k\lambda^{2}+\sigma^{2}\right)^{2}}\mathbf{i}^{T}\Xi\mathbf{i} & = & k^{2}\frac{\overline{\rho}}{(i^{T}\Phi i)^{2}}\mathbf{i}^{T}\Xi\mathbf{i}\\
 & = & \alpha_{s}\frac{\mathbf{i}^{T}\Xi\mathbf{i}}{\mathbf{i}^{T}\Psi\mathbf{i}}
\end{eqnarray*}

\end_inset

Here 
\begin_inset Formula $\alpha_{s}$
\end_inset

 is the standardized alpha based on the polychoric correlation coefficient.
\end_layout

\begin_layout Subsection
Spearman
\end_layout

\begin_layout Standard
Define the two congeneric models 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{1}=\lambda_{1}X_{1}+\diag\sigma_{1}^{2}\epsilon_{1},\quad Z_{2}=\lambda_{2}X_{2}+\diag\sigma_{2}^{2}\epsilon_{2},
\]

\end_inset

where 
\begin_inset Formula $\diag x$
\end_inset

 has 
\begin_inset Formula $x$
\end_inset

 on its diagonal and 
\begin_inset Formula $0$
\end_inset

s everywhere else.
 
\end_layout

\begin_layout Standard
Define the predictors
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\hat{Z}_{1}=\sum_{i=1}^{k_{1}}w_{1i}X_{1i},\quad\hat{Z}_{2}=\sum_{i=1}^{k_{1}}w_{2i}X_{2i}
\]

\end_inset

Then
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
\Cov(\hat{Z}_{1},\hat{Z}_{2}) & = & =\Cov(\sum_{i=1}^{k_{1}}w_{1i}X_{1i},\sum_{j=1}^{k_{2}}w_{2i}X_{1j})=\sum_{i,j}w_{1i}w_{2j}\Cov(X_{i}^{1},X_{j}^{2}).
\end{eqnarray*}

\end_inset

It follows that
\begin_inset Formula 
\begin{eqnarray*}
\Cov(X_{i}^{1},X_{j}^{2}) & = & \Cov(\lambda_{1i}Z_{1}+\sigma_{1i}\epsilon_{1i},\lambda_{2j}Z_{2}+\sigma_{2j}\epsilon_{2j})=\lambda_{1i}\lambda_{2j}\Cor(Z^{1},Z^{2}),
\end{eqnarray*}

\end_inset

and therefore,
\begin_inset Formula 
\[
\Cov(\hat{Z}_{1},\hat{Z}_{2})=\Cor(Z_{1},Z_{2})w_{1}^{T}\lambda_{1}w_{2}^{T}\lambda_{2}
\]

\end_inset

Since variance of 
\begin_inset Formula $\hat{Z}_{i}$
\end_inset

 is 
\begin_inset Formula $(w_{i}^{T}\lambda_{i})^{2}+w^{T}\diag\sigma_{1}^{2}w$
\end_inset

, it follows that 
\begin_inset Formula $\Cor(\hat{Z}_{1},\hat{Z}_{2})=\Cor(Z_{1},Z_{2})R_{1}^{1/2}R_{2}^{1/2}$
\end_inset

, as claimed.
\end_layout

