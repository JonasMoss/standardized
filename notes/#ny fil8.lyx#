\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
w_{0} & = & \frac{\overline{w\lambda}}{k\overline{w\lambda}^{2}+\overline{w^{2}\sigma^{2}}}\\
 & = & \frac{\overline{\lambda^{2}\sigma^{-2}}}{k\overline{\lambda^{2}/\sigma^{2}}^{2}+\overline{\lambda^{2}\sigma^{-2}}}=\frac{1}{1+k\lambda^{2}\sigma^{-2}}
\end{eqnarray*}

\end_inset


\end_layout

